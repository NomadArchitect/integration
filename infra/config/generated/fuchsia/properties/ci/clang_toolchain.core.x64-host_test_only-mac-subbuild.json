{
  "$recipe_engine/swarming": {
    "server": "https://chromium-swarm.appspot.com"
  },
  "recipe": "fuchsia/build",
  "recipes_host_override": "fuchsia.googlesource.com",
  "recipes_integration_ref_override": "refs/heads/main",
  "spec": {
    "artifact_gcs_bucket": "fuchsia-artifacts",
    "build": {
      "environment_tags": [
        "mac"
      ],
      "fint_params_path": "integration/infra/config/generated/fuchsia/specs/ci/clang_toolchain.core.x64-host_test_only-mac.fint.textproto",
      "run_tests": true,
      "stats_gcs_bucket": "fuchsia-artifacts",
      "upload_results": true
    },
    "checkout": {
      "manifest": "flower",
      "project": "integration",
      "remote": "https://fuchsia.googlesource.com/integration",
      "upload_results": true
    },
    "gcs_bucket": "fuchsia-build",
    "test": {
      "affected_tests_multiply_threshold": 3,
      "default_service_account": "artifact-readers@fuchsia-infra.iam.gserviceaccount.com",
      "max_attempts": 1,
      "max_attempts_per_test": 3,
      "pave": true,
      "pool": "fuchsia.tests",
      "swarming_expiration_timeout_secs": 18000,
      "swarming_io_timeout_secs": 300,
      "target_shard_duration_secs": 540,
      "timeout_secs": 2400,
      "upload_results": true
    }
  }
}