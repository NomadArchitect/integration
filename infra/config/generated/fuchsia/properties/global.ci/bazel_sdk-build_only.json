{
  "$fuchsia/rbe": {
    "instance": "projects/rbe-fuchsia-prod/instances/default"
  },
  "recipe": "fuchsia/build",
  "recipes_use_py3": true,
  "spec": {
    "artifact_gcs_bucket": "fuchsia-artifacts",
    "build": {
      "bazel_sdk_cipd_package": "fuchsia/sdk/fuchsia-bazel-rules",
      "enable_sandboxing": true,
      "fint_params_path": "integration/infra/config/generated/fuchsia/fint_params/global.ci/bazel_sdk-build_only.textproto"
    },
    "checkout": {
      "manifest": "flower",
      "project": "integration",
      "remote": "https://fuchsia.googlesource.com/integration"
    },
    "gcs_bucket": "fuchsia-build"
  }
}