  "tools": [
    {
      "cipd_package": "fuchsia/tools/futility/${platform}",
      "name": "futility"
    },
    {
      "cipd_package": "fuchsia/tools/json_validator/${platform}",
      "name": "json_validator"
    },
    {
      "gcs_bucket": "fuchsia-ffx",
      "gcs_path": "ffx-linux-x64",
      "name": "ffx"
    }
  ]
}