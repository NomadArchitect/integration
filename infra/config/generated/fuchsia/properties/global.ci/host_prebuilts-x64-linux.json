{
  "$recipe_engine/swarming": {
    "server": "https://chromium-swarm.appspot.com"
  },
  "enable_property_versioning": true,
  "experimental.rbe_cas": true,
  "fint_params_path": "integration/infra/config/generated/fuchsia/specs/global.ci/host_prebuilts-x64-linux.fint.textproto",
  "manifest": "flower",
