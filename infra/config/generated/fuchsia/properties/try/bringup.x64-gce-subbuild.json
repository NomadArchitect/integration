{
  "$recipe_engine/swarming": {
    "server": "https://chromium-swarm.appspot.com"
  },
  "experimental.test_on_gce": true,
  "recipe": "fuchsia/build",
  "spec": {
    "artifact_gcs_bucket": "fuchsia-artifacts",
    "build": {
      "fint_params_path": "integration/infra/config/generated/fuchsia/fint_params/try/bringup.x64-gce.textproto",
      "run_tests": true,
      "stats_gcs_bucket": "fuchsia-artifacts"
    },
    "checkout": {
      "manifest": "flower",
      "project": "integration",
      "remote": "https://fuchsia.googlesource.com/integration"
    },
    "gcs_bucket": "fuchsia-build",
    "test": {
      "affected_tests_multiply_threshold": 3,
      "default_service_account": "artifact-readers@fuchsia-infra.iam.gserviceaccount.com",
      "gce_mediator": {
        "cloud_project": "fuchsia-testing-vms",
        "endpoint": "fuchsia-gce-mediator-prod.uc.r.appspot.com",
        "machine_shape": "n2-standard-4"
      },
      "max_attempts_per_test": 3,
      "per_test_timeout_secs": 300,
      "pool": "fuchsia.tests",
      "swarming_expiration_timeout_secs": 18000,
      "swarming_io_timeout_secs": 360,
      "target_shard_duration_secs": 540,
      "targets_serial": true,
      "timeout_secs": 1800,
      "upload_results": true,
      "use_runtests": true
    }
  }
}