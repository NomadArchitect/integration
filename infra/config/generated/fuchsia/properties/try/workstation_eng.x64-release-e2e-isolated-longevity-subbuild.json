{
  "$fuchsia/autocorrelator": {
    "ci_bucket": "global.ci"
  },
  "$fuchsia/rbe": {
    "instance": "projects/rbe-fuchsia-prod/instances/default"
  },
  "recipe": "fuchsia/build",
  "recipes_use_py3": true,
  "spec": {
    "artifact_gcs_bucket": "fuchsia-artifacts",
    "build": {
      "environment_tags": [
        "e2e-isolated-longevity"
      ],
      "fint_params_path": "integration/infra/config/generated/fuchsia/fint_params/try/workstation_eng.x64-release-e2e-isolated-longevity.textproto",
      "incremental": true,
      "run_tests": true
    },
    "checkout": {
      "manifest": "flower",
      "project": "integration",
      "remote": "https://fuchsia.googlesource.com/integration"
    },
    "gcs_bucket": "fuchsia-build",
    "test": {
      "affected_tests_multiply_threshold": 50,
      "default_service_account": "artifact-readers@fuchsia-infra.iam.gserviceaccount.com",
      "max_attempts": 2,
      "max_attempts_per_test": 1,
      "max_shard_size": 1,
      "max_shards_per_env": 30,
      "pave": true,
      "per_shard_package_repos": true,
      "per_test_timeout_secs": 7200,
      "pool": "fuchsia.tests",
      "retry_task_on_test_failure": true,
      "skip_unaffected_tests": true,
      "swarming_expiration_timeout_secs": 18000,
      "swarming_io_timeout_secs": 7260,
      "timeout_secs": 10800,
      "upload_results": true,
      "use_cas_for_images": true
    }
  }
}