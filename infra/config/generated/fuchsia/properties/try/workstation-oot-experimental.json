{
  "build_command": {
    "args": [
      "-o",
      "{build_dir}",
      "-v",
      "{placeholder_version}"
    ],
    "path": "scripts/build.sh"
  },
  "jiri_manifest": ".manifest",
  "jiri_project": "workstation",
  "recipe": "build_test_upload",
  "recipes_use_py3": true,
  "remote": "https://fuchsia.googlesource.com/workstation",
  "upload_command": {
    "args": [
      "-b",
      "fuchsia-artifacts",
      "-n",
      "{upload_namespace}"
    ],
    "path": "scripts/upload.sh"
  }
}