{
  "build_command": {
    "path": "scripts/build.sh"
  },
  "cipd_manifest_commands": [
    {
      "command": {
        "args": [
          "-o",
          "{build_dir}"
        ],
        "path": "scripts/emit_cipd_manifest.sh"
      },
      "package": "fuchsia/drivers/backlight/x64"
    },
    {
      "command": {
        "args": [
          "-o",
          "{build_dir}"
        ],
        "path": "scripts/emit_debug_symbol_cipd_manifest.sh"
      },
      "package": "fuchsia/drivers/backlight/debug-symbols-x64"
    }
  ],
  "download_command": {
    "path": "scripts/bootstrap.sh"
  },
  "recipe": "build_test_upload",
  "recipes_use_py3": true,
  "remote": "https://fuchsia.googlesource.com/drivers/misc/google/backlight"
}