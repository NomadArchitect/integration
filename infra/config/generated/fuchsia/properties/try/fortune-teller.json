{
  "build_args": [
    "--config=fuchsia_x64",
    "//src:samples_repository",
    "//src:tests"
  ],
  "cipd_manifest_commands": [
    {
      "command": {
        "args": [
          "-o",
          "{build_dir}"
        ],
        "path": "scripts/emit_cipd_manifest.sh"
      },
      "package": "fuchsia/sdk-samples/fortune-teller/x64"
    },
    {
      "command": {
        "args": [
          "-o",
          "{build_dir}"
        ],
        "path": "scripts/emit_debug_symbol_cipd_manifest.sh"
      },
      "package": "fuchsia/sdk-samples/fortune-teller/debug-symbols-x64"
    }
  ],
  "default_service_account": "artifact-readers@fuchsia-infra.iam.gserviceaccount.com",
  "recipe": "bazel_build_test_upload",
  "recipes_use_py3": true,
  "remote": "https://fuchsia.googlesource.com/sdk-samples/fortune-teller",
  "test_manifest_command": {
    "path": "scripts/emit_test_manifest.sh"
  },
  "testing_pool": "fuchsia.tests"
}